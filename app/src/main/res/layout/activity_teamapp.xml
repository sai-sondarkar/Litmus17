<android.support.v4.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="10dp">
        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            android:id="@+id/linearLayout3"
            android:background="@drawable/animation_list">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                app:layout_scrollFlags="scroll|enterAlways"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

        </android.support.design.widget.AppBarLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:weightSum="2"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#d7ccc8">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="sans-serif-light"
                        android:text="Mr.Director"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/ravindra">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="sans-serif-light"
                        android:text="Mr. Ravindra Prajapati"
                        android:gravity="center"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Developer_OP"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center">
                    </TextView>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/sai"
                        android:id="@+id/imageView">
                    </ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="SaiKiran"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:weightSum="2"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#d7ccc8">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Mr.Java"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/ajitm">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Ajit Mourya"
                        android:gravity="center"
                        android:fontFamily="sans-serif-light"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Mr. UI/UX"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/abhijeet">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Abhijeet Prusty"
                        android:gravity="center"
                        android:fontFamily="sans-serif-light"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:weightSum="2"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#d7ccc8">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="sans-serif-light"
                        android:text="Mr.Memory_Saver"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/shivam">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="sans-serif-light"
                        android:text="Shivam Pathak"
                        android:gravity="center"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Mr.Scripter"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center"
                        android:id="@+id/textView23">
                    </TextView>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/harshiv">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Harshiv Patel"
                        android:gravity="center"
                        android:fontFamily="sans-serif-light"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="270dp"
            android:weightSum="2"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#d7ccc8">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="sans-serif-light"
                        android:text="Mr.Data_Manager"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/rahul">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Rahul Chokshi"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Mr.Animator"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center">
                    </TextView>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="@drawable/harshraj">
                    </ImageView>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="Harshraj"
                        android:fontFamily="sans-serif-light"
                        android:gravity="center"
                        android:background="#ffffff">
                    </TextView>
                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>